{
  "routes": [
      { "src": "/playlist/(?<dataset>[^/]+)/(?<id>[^/]+)/edit", "dest": "/playlist/_dataset/_id/edit.html" },
      { "src": "/playlist/(?<dataset>[^/]+)/new", "dest": "/playlist/_dataset/new.html" },
      { "src": "/playlist/(?<dataset>[^/]+)/(?<id>[^/]+)", "dest": "/playlist/_dataset/_id.html" }
  ],
  "build": {
      "env": {
        "GRIDSOME_OAUTH_CALLBACK": "@oauth_callback"
      }
  }
}
